{
  "name": "@coinflowlabs/react",
  "version": "5.0.8",
  "description": "React Component for Coinflow Withdraw",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/coinflow-labs-us/coinflow-react"
  },
  "keywords": [
    "react",
    "typescript",
    "solana",
    "web3"
  ],
  "scripts": {
    "clean": "rimraf build && rimraf ./src/common",
    "build": "npm run codegen && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir build/cjs",
    "codegen": "cp -r '../../packages/lib-common/src/.' ./src/common/"
  },
  "files": [
    "build",
    "README.md"
  ],
  "dependencies": {
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "lz-string": "^1.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "eslint-config-react-app": "^7.0.1",
    "typescript": "^5.6.3",
    "csstype": "^3.1.3"
  },
  "peerDependencies": {
    "@solana/web3.js": ">=1.54.0",
    "bs58": "~5.0.0",
    "react": ">=16",
    "@coinflowlabs/lib-common": "*"
  },
  "peerDependenciesMeta": {
    "@solana/web3.js": {
      "optional": true
    },
    "bs58": {
      "optional": true
    },
    "@coinflowlabs/lib-common": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
